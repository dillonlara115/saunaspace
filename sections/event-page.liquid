
	<section class="sec1000">

			<div class="event-top">
				<div class="event-top-text">
					<h1 class="et-title">
                        {%- if section.settings.page.title != blank -%}
                            {{ section.settings.page.title | escape }}
                        {%- else -%}
                            {{ 'sections.page.title' | t }}
                        {%- endif -%}
                    </h1>
					<div class="et-dates">
                        {% unless page.metafields.start-date.startdate == blank %}
                            <span class="eventStartDate">
                                {{page.metafields.start-date.startdate}}
                            </span>
                        {% endunless %}
                        {% unless page.metafields.end-date.enddate == blank %}
                        - <span class="eventEndDate">{{page.metafields.end-date.enddate}}</span>. 
                        {% endunless %}
                        <span class="eventStartTime">8:00am</span> - <span class="eventEndTime">5:00pm</span></div>
					<div class="et-location etica">
						<span class="eventVenue">The Angel</span><br />
						<span class="eventAddress">52 Upper Street</span> <span class="eventCity">London</span> <span class="eventState"></span> <span class="eventZip">N1 0HQ</span> <span class="eventCountry">UK</span> 
					</div>
				</div>
				<a href="https://summit.healthoptimisation.com/" target="_blank" class="et-link">Register</a>
			</div>
			<div class="event-box">
				<div class="event-text">
					<div class="event-title">Event Details</div>
					{{ section.settings.page.content }}
					<div class="event-link-div"><a href="https://summit.healthoptimisation.com/" target="_blank" class="event-link">Register <svg xmlns="http://www.w3.org/2000/svg" width="14.759" height="7.753" overflow="visible"><g fill="none" stroke="#000"><path d="M0 3.876h14M10 .376l4 3.5-4 3.5"/></g></svg></a></div>
				</div>
				<div class="events-img"><img src="../img/events/health-optimization-summit-2022.jpg" /></div>
			</div>
	</section>
	
	<section class="sec1000">
		<div class="event-back">
			<a href="/pages/events" class="event-link"><svg xmlns="http://www.w3.org/2000/svg" width="14.759" height="7.753" overflow="visible" style="transform: rotate(180deg);"><g fill="none" stroke="#000"><path d="M0 3.876h14M10 .376l4 3.5-4 3.5"/></g></svg> Back to Events</a>
		</div>
	</section>


    {% schema %}
{
  "name": "t:sections.main-page.name",
  "tag": "section",
  "class": "section",
  "settings": [
    {
      "type": "header",
      "content": "t:sections.all.padding.section_padding_heading"
    },
    {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_top",
      "default": 36
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_bottom",
      "default": 36
    }
  ]
}
{% endschema %}
