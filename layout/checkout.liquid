                      console.log('EXW');
                      $(this).append('<svg aria-hidden="true" data-exw focusable="false" data-prefix="fas" data-icon="info-circle" class="masterTooltip svg-inline--fa fa-info-circle fa-w-16" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><path fill="currentColor" d="M256 8C119.043 8 8 119.083 8 256c0 136.997 111.043 248 248 248s248-111.003 248-248C504 119.083 392.957 8 256 8zm0 110c23.196 0 42 18.804 42 42s-18.804 42-42 42-42-18.804-42-42 18.804-42 42-42zm56 254c0 6.627-5.373 12-12 12h-88c-6.627 0-12-5.373-12-12v-24c0-6.627 5.373-12 12-12h12v-64h-12c-6.627 0-12-5.373-12-12v-24c0-6.627 5.373-12 12-12h64c6.627 0 12 5.373 12 12v100h12c6.627 0 12 5.373 12 12v24z" style="width: 15px; margin-left: .4rem;" ></path></svg>');
                      $('[data-exw]').on('click', function(){
                         console.log('exw clicked');
                         $('#popup_default').openPopup();
                      });
                      $(this).parent('.radio__label').find('.content-box__emphasis').text('Varies');
                      $('.edit_checkout').find('.step__sections').append('<label for="attributes[Agree-TOS]" class="ddu-label"><input type="checkbox" class="checkout-checkbox" name="attributes[Agree-TOS]" value="Yes" required>I understand that I must pay all Duties, Taxes and Fees when my package arrives to my country.<svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="info-circle" class="masterTooltip svg-inline--fa fa-info-circle fa-w-16" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><path fill="currentColor" d="M256 8C119.043 8 8 119.083 8 256c0 136.997 111.043 248 248 248s248-111.003 248-248C504 119.083 392.957 8 256 8zm0 110c23.196 0 42 18.804 42 42s-18.804 42-42 42-42-18.804-42-42 18.804-42 42-42zm56 254c0 6.627-5.373 12-12 12h-88c-6.627 0-12-5.373-12-12v-24c0-6.627 5.373-12 12-12h12v-64h-12c-6.627 0-12-5.373-12-12v-24c0-6.627 5.373-12 12-12h64c6.627 0 12 5.373 12 12v100h12c6.627 0 12 5.373 12 12v24z" style="width: 15px; margin-left: .4rem;" data-ddu=""></path></svg></label>');
                     $('[data-ddu]').on('click', function(){
                         console.log('ddu clicked');
                         $('#popup_ddu').openPopup();
                      }); 
                   } 
                });
              
              $( ".input-radio" ).change(function() {
                if($('input[value="shopify-Customer-Arranged%20Shipment-0.00"]').is(':checked')){
                    console.log('CAS is checked');
                      $('#popup_default').openPopup();
                }
              });
               if($('input[value="shopify-Customer-Arranged%20Shipment-0.00"]').is(':checked')){
                    console.log('CAS is checked');
                      $('#popup_default').openPopup();
                }
             }, 2000);
    
              setTimeout(function(){ 
                 
                if($("#tooltip").length < 1) {
                  console.log('taxes');
                  $(".total-line--taxes")
                    .find(".total-line__name")
                    .append('<svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="info-circle" class="masterTooltip svg-inline--fa fa-info-circle fa-w-16" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><path fill="currentColor" d="M256 8C119.043 8 8 119.083 8 256c0 136.997 111.043 248 248 248s248-111.003 248-248C504 119.083 392.957 8 256 8zm0 110c23.196 0 42 18.804 42 42s-18.804 42-42 42-42-18.804-42-42 18.804-42 42-42zm56 254c0 6.627-5.373 12-12 12h-88c-6.627 0-12-5.373-12-12v-24c0-6.627 5.373-12 12-12h12v-64h-12c-6.627 0-12-5.373-12-12v-24c0-6.627 5.373-12 12-12h64c6.627 0 12 5.373 12 12v100h12c6.627 0 12 5.373 12 12v24z" style="width: 15px; margin-left: .4rem;" data-tip=""></path></svg><div id="tooltip" class="tooltip tooltip-tax tooltip-checkout">The shipping address determines taxability of your order.');
                  }
    
    
               var toolTips = {
                  timeout: null,
                  html: '<div class="tooltip-checkout"></div>',
    
                init: function(){
                   
                   var event = ('ontouchstart' in window) ? 'click' : 'mouseenter mouseleave';
    
                   $('[data-tip]').on(event, function(){
                      $('.tooltip-checkout').toggleClass('tooltip-is-active');
                   });
                   $('[data-pickup]').on(event, function(){
                      $('.tooltip-pickup').toggleClass('tooltip-is-active');
                   });
                   $('[data-exu]').on(event, function(){
                      $('.tooltip-exw').toggleClass('tooltip-is-active');
                   });
    //                $('[data-ddu]').on(event, function(){
    //                   $('.tooltip-ddu').toggleClass('tooltip-is-active');
    //                });
    
                },
    
               show: function(){
                  $('.tooltip-checkout').css({opacity:0});
                  clearTimeout(toolTips.timeout);
                  $('.tooltip-checkout').html($(this).data('tip'));
    
                  var offset = $(this).offset(),
                     height = $(this).outerHeight()+15,
                     parWidth = $(this).outerWidth()/2,
                     width  = (($('.tooltip-checkout').outerWidth())/2)-parWidth;
                  $('.tooltip-checkout').css({top:offset.top+height, left:0, opacity:1});
               },
    
               hide: function(){
                  toolTips.timeout = setTimeout(function(){
                     $('.tooltip-checkout').css({opacity:0});
                  }, 150);
               }
            }
    
          
             toolTips.init();
                
           }, 2500);
            document.addEventListener('DOMContentLoaded', function() {
                 console.log('running');
                   document.querySelector('img[alt="Bread"]').setAttribute('src', 'https://cdn.shopify.com/s/files/1/0026/7229/6025/files/sauna-now-finance-over-time.png?v=1640116210');
                });
          })(jQuery);
        </script>
    
